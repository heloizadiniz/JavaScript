let contador = 1;
while (contador <= 10) {
    console.log(contador);
    contador++;
}
let contador = 10;
while (contador >= 0){
    console.log(contador);
    contador--;
}

let numeroMaximo = prompt("Digite um número da contagem ");
while (numeromaximo >= 0);{
    console.log(numeromaximo);
    numeromaximo--;
}

let numeroMaximo = prompt("Digite um número");
while (numeromaximo = 0);{
    console.log(numeroMaximo);
    numeromaximo++;
}






